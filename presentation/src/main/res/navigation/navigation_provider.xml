<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_provider"
    app:startDestination="@id/providerFragmentV2">

    <fragment
        android:id="@+id/providerFragmentV2"
        android:name="io.forus.me.android.presentation.view.screens.provider_v2.provider_main.ProviderFragmentV2"
        android:label="ProviderFragmentV2"
        tools:layout="@layout/fragment_providerv2"
        >
        <action
            android:id="@+id/action_providerFragmentV2_to_actionPaymentFragment"
            app:destination="@id/actionPaymentFragment" />
    </fragment>

    <fragment
        android:id="@+id/actionPaymentFragment"
        android:name="io.forus.me.android.presentation.view.screens.vouchers.voucher_with_actions.payment.ActionPaymentFragment"
        android:label="ActionPaymentFragment"
        tools:layout="@layout/fragment_action_payment"
        >
        <argument
            android:name="voucherAddress"
            app:argType="string" />

        <argument
            android:name="product"
            app:argType="io.forus.me.android.presentation.view.screens.vouchers.voucher_with_actions.payment.ProductSerializable"
            />
    </fragment>
</navigation>